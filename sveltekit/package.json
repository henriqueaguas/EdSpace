{
	"name": "edspace-server",
	"type": "module",
	"version": "1.0.0",
	"scripts": {
		"dev": "vite dev --host",
		"build": "npm i && prisma generate --data-proxy && npm run prisma:inline && vite build",
		"preview": "NODE_ENV=production vite preview",
		"deploy:win": "cd .. & vercel deploy & vercel --prod",
		"test": "cross-env NODE_ENV=test npm run db:clear && vitest run && cross-env NODE_ENV=test npm run db:clear",
		"pg": "tsx tests/_playground/index.ts",
		"db:start": "docker start edspace-db",
		"db:remove:test": "docker rm -f edspace-db",
		"db:create:test": "npm run db:remove:test && docker run --name edspace-db -e POSTGRES_USER=dbuser -e POSTGRES_PASSWORD=dbpass -e POSTGRES_DB=test -p 5432:5432 -d postgres && docker exec edspace-db sh -c \"while ! pg_isready -q; do sleep 1; done\" && npm run db:init:test && npm run db:pull:test",
		"db:init:test": "cross-env SCRIPT_CALL=true INIT_PROD=false tsx prisma/scripts/initDB.ts",
		"db:init:prod": "cross-env SCRIPT_CALL=true INIT_PROD=true tsx prisma/scripts/initDB.ts",
		"db:clear": "cross-env SCRIPT_CALL=true tsx prisma/scripts/clearData.ts",
		"db:mockdata:test": "cross-env NODE_ENV=test tsx tests/_mock-data/generate.ts",
		"db:pull:test": "prisma db pull && prisma generate",
		"prisma:inline": "cp node_modules/.prisma/client/*.js node_modules/@prisma/client && cp prisma/schema.prisma node_modules/@prisma/client",
		"db:view": "prisma studio",
		"tc": "tsc --noEmit && svelte-check",
		"api:docs": "tsx src/routes/api/docs.ts --out=docs/api-docs.md",
		"gc:cors": "gsutil cors set cloud_cors_cfg.json gs://edspace-post-pages",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@auth/core": "^0.5.1",
		"@auth/sveltekit": "^0.3.0",
		"@babel/preset-typescript": "^7.21.4",
		"@fontsource/fira-mono": "^4.5.10",
		"@google-cloud/storage": "^6.9.4",
		"@neoconfetti/svelte": "^1.0.0",
		"@next-auth/prisma-adapter": "^1.0.5",
		"@prisma/client": "^5.0.0",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-node": "^1.2.4",
		"@sveltejs/adapter-vercel": "^3.0.0",
		"@sveltejs/kit": "^1.11.0",
		"@types/cookie": "^0.5.1",
		"@types/jsdom": "^21.1.1",
		"@types/next-auth": "^3.15.0",
		"@types/qrcode": "^1.5.0",
		"@types/uuid": "^9.0.1",
		"@vitest/ui": "^0.30.1",
		"autoprefixer": "^10.4.14",
		"concurrently": "^8.0.1",
		"cross-env": "^7.0.3",
		"dotenv": "^16.0.3",
		"fs-jetpack": "^5.1.0",
		"jsdom": "^21.1.1",
		"lucide-svelte": "^0.214.0",
		"next-auth": "^4.20.1",
		"postcss": "^8.4.21",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"prisma": "^5.0.0",
		"qrcode": "^1.5.3",
		"stream": "^0.0.2",
		"svelte": "^3.54.0",
		"svelte-check": "^3.2.0",
		"tailwindcss": "^3.2.7",
		"tsc-alias": "^1.8.5",
		"tsx": "^3.12.6",
		"typescript": "^4.4.4",
		"uuid": "^9.0.0",
		"vite": "^4.0.0",
		"vitest": "^0.29.7",
		"zod": "^3.21.4"
	},
	"dependencies": {
		"@types/pg": "^8.10.1",
		"@types/prompt-sync": "^4.2.0",
		"daisyui": "^2.51.5",
		"pg": "^8.11.0",
		"pg-promise": "^11.5.0",
		"prompt-sync": "^4.2.0",
		"redis": "^4.6.7",
		"reflect-metadata": "^0.1.13",
		"svelte-french-toast": "^1.0.4-beta.0",
		"svelte-share-buttons-component": "^1.5.5",
		"tsyringe": "^4.8.0",
		"vercel": "^30.2.3",
		"zero-md": "^2.4.0"
	}
}
